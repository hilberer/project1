age = localStorage.getItem('credentials')\r\n                if(storage === null){\r\n                    return {username: '', password: ''}\r\n                }\r\n                const user: loginCredentials = JSON.parse(storage)\r\n                return user\r\n\r\n            })\r\n        }\r\n    }, [setAuthUser])\r\n\r\n    return (\r\n        <BrowserRouter>\r\n        {children}\r\n            <Switch>\r\n                <Route exact path={RoutingPath.home} component={Home} />\r\n                <Route exact path={RoutingPath.signin} component={blockRouteIfAuthenticated(SignInView, Home)} />\r\n                <Route exact path={RoutingPath.newrecipe} component={NewRecipe} />\r\n                <Route exact path={RoutingPath.addingredient} component={AddIngredient} />\r\n                <Route component={Home} />\r\n                <Route exact path={RoutingPath.settingsView} component={SettingsView} />\r\n            </Switch>\r\n        </BrowserRouter>\r\n    )\r\n}","/mnt/c/Users/hilberer/Documents/git/project1/client/src/components/navigation/Navigation.tsx",[],"/mnt/c/Users/hilberer/Documents/git/project1/client/src/shared/provider/StarwarsCharProvider.tsx",[],"/mnt/c/Users/hilberer/Documents/git/project1/client/src/shared/provider/UserProvider.tsx",[],"/mnt/c/Users/hilberer/Documents/git/project1/client/src/shared/provider/PizzaNameProvider.tsx",[],"/mnt/c/Users/hilberer/Documents/git/project1/client/src/routes/RoutingPath.ts",[],"/mnt/c/Users/hilberer/Documents/git/project1/client/src/pages/Home.tsx",["118","119"],"import './Home.css'\r\nimport { AddButton } from '../components/addbutton/AddButton'\r\nimport { Slider } from '../components/slider/Slider'\r\nimport { useContext } from 'react'\r\nimport { pizzaContext } from '../shared/provider/PizzaNameProvider'\r\nimport { Pizza, Ingredient } from '../shared/interface/Recipe'\r\nimport { StarwarsCharContext } from '../shared/provider/StarwarsCharProvider'\r\n\r\n\r\n\r\nexport const Home = () => {\